{
    "name": "horizon-main-server",
    "version": "3.0.0",
    "description": "",
    "main": "app.js",
    "scripts": {
        "test": "snyk test",
        "k6": "k6 run test/load-test.js",
        "process_k6": "jq '. | select(.type=='Point' and .metric == 'http_req_duration' and .data.tags.status >= '200')' test/load-test.json",
        "debug": "node-inspect app.js",
        "inspect": "node --inspect app.js",
        "lint": "eslint ./",
        "dev": "nodemon app.js",
        "start": "cross-env NODE_ENV=production node --max-old-space-size=256 app.js",
        "server-only": "nodemon server-only.js",
        "build": "npm run grunt-all",
        "grunt-all": "grunt cssmin && grunt uglify",
        "grunt-css": "grunt cssmin",
        "grunt-js": "grunt uglify",
        "obfuscate": "webpack ./webpack.config.js",
        "prepare": "husky install"
    },
    "keywords": [
        "horizon",
        "lenses",
        "vision",
        "single-vision",
        "bifocal",
        "industry",
        "startups"
    ],
    "author": {
        "name": "Ayk"
    },
    "license": "ISC",
