        "connect-flash": "^0.1.1",
        "connect-mongodb-session": "^3.1.1",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "csurf": "^1.10.0",
        "dotenv": "^8.2.0",
        "ejs": "^3.1.8",
