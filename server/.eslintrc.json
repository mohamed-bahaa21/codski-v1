{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "es6": true,
        "esversion": 6,
        "jquery": true
    },
    "parserOptions": {
        "ecmaVersion": 12
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}