<!-- Start Recent News -->
<section id="blog" class="padding-100">
  <div class="container">
    <div class="row">
      <div class="text-center col-12 section-title" data-aos="fade-zoom-in">
        <h2 class="section-header-a">
          <span class="section-header-1" style="color: white;">Latest</span>
          <span class="section-header-2">News</span>
        </h2>
        <div class="space-25"></div>
        <!-- <p class="sec-desc" style="color: white;">
                            Lorem ipsum madolor sit amet, consectetur adipisicing elit, sed do
                            eiusmod tempor coli incidit labore lorem ipsum amet madolor sit
                            amet.
                        </p> -->
      </div>
    </div>
    <div class="row align-items-center">
      <div class="col-12">
        <section id="news" class="">
          <div class="news-slider">
            <div class="news-slider__wrp swiper-wrapper">
              <% blogs.forEach(function(blog) { %>
              <div class="news-slider__item swiper-slide">
                <div class="news-slider__img">
                  <img src="<%= blog.thumb_img %>" alt="" />
                </div>
                <div class="news-slider__content">
                  <span class="news-slider__code">
                    <%= blog.date %>
                  </span>
                  <div class="news-slider__title">
                    <%= blog.title %>
                  </div>
                  <div class="news-slider__text">
                    <%= blog.summary %>
                  </div>
                  <a href="/news/<%= blog.url %>" class="btn-primary">READ MORE</a>
                </div>
              </div>
              <% }) %>
            </div>
            <div class="news-slider__pagination"></div>
            <!-- <a class="" style="float: right; margin: -1rem; background-color: white;" href="#">View all
                                >></a> -->
          </div>
        </section>
      </div>

    </div>
  </div>
</section>
<!-- End Recent News  -->
<!-- ============================================================================ -->





